<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android" android:versionCode="3" android:versionName="1.1.0" package="com.dapath.dapatha">
    <uses-sdk android:minSdkVersion="11" android:targetSdkVersion="21" />
